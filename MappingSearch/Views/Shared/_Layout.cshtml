<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet/less" href="~/Content/Site.less" type="text/css" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/backbone")
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Scripts/libs/less-1.3.3.min.js" type="text/javascript"></script>
</head>
<body>
    <div id="page" class="rounded-corners-all">
    <div class="clearfix">
        <nav>
            <ul class="navigation">
                <li><a href="/">Home</a></li>
                <li><a href="/AddLocation/Index">Add Location</a></li>
            </ul>
        </nav>
    </div>
    @RenderBody()

    @RenderSection("scripts", required: false)
    </div>
</body>
</html>
