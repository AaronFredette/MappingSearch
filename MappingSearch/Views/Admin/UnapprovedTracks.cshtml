@model  MappingSearch.Models.ViewModels.Admin.AdminApprovalViewModel
@{
    ViewBag.Title = "UnapprovedTracks";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="unapprovedTracks">
<h2>Unapproved Tracks</h2>
<form>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Is Approved</th>
            <th>Name</th>
            <th>State</th>
            <th>Description</th>
            <th>Image</th>
            <th>Site</th>
            <th>Subcategory</th>
            <th>Submitted By</th>
        </tr>
    </thead>
    <tbody>
        @{var tracks = Model.UnapprovedTracks;}
    @for (int i = 0; i < tracks.Count;i++ )
    {
        
        <tr>
            <td><input type="checkbox" data-trackid="@tracks[i].Id"</td>
            <td>@tracks[i].Name</td>
            <td>@tracks[i].City</td>
            <td>@tracks[i].State</td>
            <td>@tracks[i].StreetAddress</td>
            <td>@tracks[i].Zip</td>
            <td>@tracks[i].TrackLength</td>
            <td>@tracks[i].</td>
            <td><a href="@tracks[i].Url">@tracks[i].Url</a></td>
            <td>@tracks[i].SubmittedBy</td>
           
        </tr>

    }
        <tr>
            <td>
                <button class="approve" value="Approve">Approve</button>
            </td>
        </tr>
</table>
</form>
    </div>